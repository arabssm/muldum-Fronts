{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@_assets/*": ["src/assets/*"],
      "@_atom/*": ["src/atom/*"],
      "@_components/*": ["src/component/*"],
      "@_componentsModal/*": ["src/component/modal/*"],
      "@_componentsOnboarding/*": ["src/component/onboarding/*"],
      "@_util/*": ["src/util/*"],
      "@_styles/*": ["src/styles/*"]
    },
    "jsx": "react-jsx",
    "lib": ["dom", "esnext"],
    "types": ["react"],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}