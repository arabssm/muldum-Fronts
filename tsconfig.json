{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@_assets/*": ["src/assets/*"],
      "@_modalImages/*": ["src/assets/modal/*"],
      "@_components/*": ["src/components/*"],
      "@_menu/*": ["src/components/Menu/*"],
      "@_modal/*": ["src/components/Modal/*"],
      "@_navbar/*": ["src/components/NavBar/*"],
      "@_notfound/*": ["src/components/NotFound/*"],
      "@_pages/*": ["src/page/*"],
      "@_main/*": ["src/page/Main/*"],
      "@_notice/*": ["src/page/Notice/*"],
      "@_styles/*": ["src/font.css"]
    },
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}